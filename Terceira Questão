/* Terceira Questão */

using System;

class Program {
  public static void Main (string[] args) {

    float a = 5, b = 3, c = 3;

    if ((a < b + c) && (b < a + c))
    {
      if (a == b && b == c)
      {
        Console.WriteLine("Triângulo Equilátero");
      }
      else if (a ==b || b == c || a == c)
      {
        Console.WriteLine("Triângulo Isósceles");
      }
      else 
      {
        Console.WriteLine("Triângulo Escaleno");
      }
    }
    else
    {
      Console.WriteLine("Não é possivel formar um triângulo");
    }
  }
  
}

/* Escaleno
Escaleno
Isósceles
Equilátero
Isósceles
*/
